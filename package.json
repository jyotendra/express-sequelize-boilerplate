{
  "name": "ec2-app",
  "version": "0.0.1",
  "description": "This project is using express and sequelize",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc.json src/*",
    "build": "cross-env NODE_ENV=development npm run clean-build && npm run lint && npm run copy-files && npm run compile && npm run set-watch",
    "build:prod": "cross-env NODE_ENV=production npm run clean-build && npm run lint && npm run copy-files && npm run compile",
    "compile": "./node_modules/.bin/gulp compile-babel",
    "start-nodemon": "cross-env NODE_ENV=development nodemon ./build/src/server.js",
    "clean-build": "./node_modules/.bin/gulp clean-build",
    "set-watch": "./node_modules/.bin/gulp set-watch",
    "serve": "npm run start-nodemon",
    "copy-files": "./node_modules/.bin/gulp copy-folders",
    "sqlz:migrate": "cross-env NODE_ENV=development ./node_modules/.bin/sequelize db:migrate",
    "sqlz:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "sqlz:migration:new": "./node_modules/.bin/sequelize migration:create",
    "sqlz:seed:new": "./node_modules/.bin/sequelize seed:generate",
    "sqlz:seed:all": "./node_modules/.bin/sequelize db:seed:all"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.4",
    "dotenv": "^8.1.0",
    "express": "4.17.1",
    "express-validator": "^6.2.0",
    "gulp-sourcemaps": "^2.6.5",
    "helmet": "^3.21.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "mysql2": "^2.1.0",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.3.0",
    "objectmodel": "^3.7.7",
    "reflect-metadata": "0.1.12",
    "sequelize": "^5.18.4",
    "source-map-support": "^0.5.16",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@types/bcrypt": "^3.0.0",
    "@types/bluebird": "3.5.27",
    "@types/chai": "4.2.3",
    "@types/express": "4.17.1",
    "@types/express-validator": "3.0.0",
    "@types/jsonwebtoken": "^8.3.3",
    "@types/lodash": "^4.14.138",
    "@types/mocha": "5.2.7",
    "@types/node": "12.7.5",
    "@types/sequelize": "^4.28.4",
    "@types/source-map-support": "^0.5.0",
    "babel-eslint": "^10.0.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cross-env": "^6.0.0",
    "eslint": "^5.16.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-nodejs": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-cli": "^2.2.0",
    "gulp-file": "^0.4.0",
    "gulp-rimraf": "^1.0.0",
    "mkdirp": "^0.5.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "sequelize-cli": "^5.5.1"
  },
