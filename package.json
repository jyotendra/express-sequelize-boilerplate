{
  "name": "express-sequelize",
  "version": "0.0.1",
  "description": "This project is using express and sequelize",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/eslint -c ./.eslintrc.json src/*",
    "build": "npm run lint && npm run compile",
    "compile": "./node_modules/.bin/babel src/ -d build -s --ignore spec.js -w -s",
    "start-nodemon": "nodemon ./build/server.js",
    "serve": "npm run build | npm run start-nodemon",
    "sqlz:migrate": "./node_modules/.bin/sequelize db:migrate",
    "sqlz:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "sqlz:migration:new": "./node_modules/.bin/sequelize migration:create",
    "sqlz:seed:new": "./node_modules/.bin/sequelize seed:generate",
    "sqlz:seed:all": "./node_modules/.bin/sequelize db:seed:all",
    "sqlz:model:new": "./node_modules/.bin/sequelize model:generate"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "bluebird": "3.5.1",
    "body-parser": "1.18.2",
    "cross-env": "5.1.3",
    "express": "4.16.2",
    "express-validator": "^5.0.2",
    "helmet": "3.11.0",
    "inversify": "4.10.0",
    "jsonwebtoken": "^8.2.0",
    "lodash": "^4.17.5",
    "pg": "7.4.1",
    "pg-hstore": "~2.3.2",
    "reflect-metadata": "0.1.12",
    "sequelize": "4.33.4",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^1.0.0",
    "@types/bluebird": "3.5.20",
    "@types/chai": "4.1.2",
    "@types/express": "4.11.1",
    "@types/express-validator": "3.0.0",
    "@types/jsonwebtoken": "7.2.5",
    "@types/lodash": "4.14.105",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.6",
    "@types/sequelize": "4.27.6",
    "@types/source-map-support": "^0.4.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.19.0",
    "eslint-config-node": "^2.0.0",
    "eslint-config-nodejs": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "mocha": "^5.0.2",
    "nodemon": "^1.15.1",
    "rimraf": "^2.6.2",
    "sequelize-cli": "^4.0.0"
  },
  "author": "",
  "license": "ISC"
}
